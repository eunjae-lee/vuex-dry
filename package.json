{
  "name": "vuex-dry",
  "version": "0.1.10",
  "description": "DRY vuex, no more repetitive codes",
  "main": "dist/vuex-dry.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "bili",
    "test": "jest",
    "autotest": "npm test -- --watch",
    "patch": "git pull && npm run test && npm version patch && npm publish && git push",
    "minor": "git pull && npm run test && npm version minor && npm publish && git push",
    "toc_document": "markdown-toc -i DOCUMENT.md --maxdepth=2",
    "doc": "node doc.js && npm run toc_document"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eunjae-lee/vuex-dry.git"
  },
  "keywords": [
    "vuex",
    "vue",
    "util",
    "state",
    "management",
    "plugin"
  ],
  "author": "Eunjae Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eunjae-lee/vuex-dry/issues"
  },
  "homepage": "https://github.com/eunjae-lee/vuex-dry#readme",
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@vue/test-utils": "^1.0.0-beta.18",
    "bili": "^3.1.2",
    "jest": "^23.1.0",
    "markdown-toc": "^1.2.0",
    "rollup-plugin-typescript2": "^0.14.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.1",
    "vue": "^2.5.16",
    "vue-template-compiler": "^2.5.16",
    "vuex": "^3.0.1"
  }
}
